{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/videos/(.*)",
      "dest": "/videos"
    },
    {
      "src": "/comments/(.*)",
      "dest": "/comments"
    },
    {
      "src": "/products/(.*)",
      "dest": "/products"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
